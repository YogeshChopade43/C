#include<stdio.h>
int main()
{
	int a[20],n,i,j,temp,b,k;
	
	printf("Enter the size of array:");
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		printf("Enter elements:");
		scanf("%d",&a[i]);
	}
	for(b=0;b<n;b++)
	{
	
	for(k=b;k<n-1;k++)
	{
	if(a[b]>a[k+1])
	{
		temp=a[b];
		a[b]=a[k+1];
		a[k+1]=temp;
	}
}
}
	for(j=0;j<n;j++)
	{
		printf("%d\t",a[j]);
		
	}
	return 0;
}
